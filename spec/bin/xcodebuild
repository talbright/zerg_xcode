#!/usr/bin/env ruby
#
# A mock xcodebuild for testing.
#

def showsdks
  puts <<EOF
Mac OS X SDKs:
        Mac OS X 10.6                   -sdk macosx10.6
        Mac OS X 10.7                   -sdk macosx10.7

iOS SDKs:
        iOS 5.1                         -sdk iphoneos5.1

iOS Simulator SDKs:
        Simulator - iOS 5.1             -sdk iphonesimulator5.1
EOF
end

showsdks if ARGV[0] == '-showsdks'

